<h1>Hello Client World</h1>

 <% flash.each do |name,msg| %>
    <%= msg %></br>
 <% end %>

<ul>
	<% @clients.each do |client| %>
		<li>
			<tr>
				<td><%="#{client.first_name} #{client.last_name} "%></td>
				<td><%="#{client.email} "%></td>
				<td><%="#{client.company_name} "%></td>
				<td><%= link_to ' Client Details', client_path(client) %></td>
	    		<td><%= link_to ' Edit Client', edit_client_path(client) %></td>
			</tr>
		</li>
	<%end%>
</ul>

